//one (concrete) subject
public class Weatherdata implenments Subject
   private ArrayList<Observer> observers;
   private temp;
   private humi;
   public Weatherdata()
     observers = new ArrayList<Observer>();
   republic registerObserver(Observer o)
   republic removeOvserver(Observer o)
   republic notifyObservers()
   republic measurementsChanged()
   republic setMeasurements
     temp = temperature;
     humi = humidity;
  
